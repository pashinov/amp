{
  "Config": {

    "Application": {
      "Name": "dragon",
      "Version": "0.0.1"
    },

    "System": {
      "Logging": {
        "Level": "info",
        "Path" : "/tmp/dragon.log"
      }
    },

    "Service": {
      "REST": {
        "Endpoint": "http://localhost:9001/api/v1"
      },
      "IoT": {
        "Connector": {
          "ZMQ": {
            "Pub": {
              "Addr": "ipc:///tmp/phoenix_sender",
              "Topic": "phoenix_sender"
            },
            "Sub": {
              "Addr": "ipc:///tmp/phoenix_receiver",
              "Topic": "phoenix_receiver",
              "Timeout": 1000
            }
          }
        }
      }
    }

  }
}
