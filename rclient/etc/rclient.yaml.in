#
#  Description: General configuration file
#

router:
  zmq:
    url:      @ROUTER_ZMQ_URL@

service:
  zmq:
    url:      @SERVICE_ZMQ_URL@
    timeout:  @SERVICE_ZMQ_TIMEOUT@

  daemon:
    pidfile:  @SERVICE_DAEMON_PIDFILE@

  channel:
    timeout:
      secs:   @SERVICE_CHANNEL_TIMEOUT_SECS@
      nanos:  @SERVICE_CHANNEL_TIMEOUT_NANOS@
